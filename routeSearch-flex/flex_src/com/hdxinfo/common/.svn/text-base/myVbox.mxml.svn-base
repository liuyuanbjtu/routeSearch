<?xml version="1.0" encoding="utf-8"?>
<mx:VBox xmlns:mx="http://www.adobe.com/2006/mxml"
		 verticalGap="2" horizontalAlign="center">
	<mx:Script>
		<![CDATA[
			[Bindable]
			public var stra:String;
			[Bindable]
			public var strb:String;
		]]>
	</mx:Script>
	<mx:Label text="{stra}"/>
	<mx:HRule width="100%"/>
	<mx:Label text="{strb}"/>
</mx:VBox>
